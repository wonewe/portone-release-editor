{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "editor.html", "use": "@vercel/static" },
    { "src": "assets/**/*", "use": "@vercel/static" },
    { "src": "api/generateReleaseNote.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/generateReleaseNote", "dest": "/api/generateReleaseNote.js" },
    { "src": "/editor", "dest": "/editor.html" }
  ]
}
